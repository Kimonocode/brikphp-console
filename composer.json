{
    "name": "kimonocode/brikphp-console",
    "description": "Console Brikphp pour controler le framework",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "kimonocode"
        }
    ],
    "autoload": {
        "psr-4": {
            "Brikphp\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Brikphp\\Console\\Tests\\" : "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "squizlabs/php_codesniffer": "^3.0",
        "react/http": "^1.11",
        "cboden/ratchet": "^0.4.4",
        "react/socket": "^1.16"
    },
    "require": {
        "symfony/console": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/yaml": "^7.2",
        "kimonocode/brikphp-filesystem": "main-dev"
    }
}
